{
  "name": "gaming-admin-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve:shell",
    "build": "node scripts/build.js",
    "build:parallel": "node scripts/build.js --parallel",
    "build:shell": "cd apps/shell && webpack --mode production",
    "build:report": "cd apps/user-report && webpack --mode production",
    "build:transaction": "cd apps/user-transaction && webpack --mode production",
    "build:profile": "cd apps/user-profile && webpack --mode production",
    "build:app-user": "cd apps/app-user && webpack --mode production",
    "serve:shell": "node scripts/serve.js shell",
    "serve:report": "node scripts/serve.js user-report",
    "serve:transaction": "node scripts/serve.js user-transaction",
    "serve:profile": "node scripts/serve.js user-profile",
    "serve:app-user": "node scripts/serve.js app-user",
    "serve:all": "concurrently \"npm run serve:shell\" \"npm run serve:report\" \"npm run serve:transaction\" \"npm run serve:profile\" \"npm run serve:app-user\"",
    "install:all": "npm install && npm run install:apps",
    "install:apps": "concurrently \"cd apps/shell && npm install\" \"cd apps/user-report && npm install\" \"cd apps/user-transaction && npm install\" \"cd apps/user-profile && npm install\" \"cd apps/app-user && npm install\"",
    "clean": "rimraf dist",
    "clean:all": "npm run clean && npm run clean:apps",
    "clean:apps": "concurrently \"cd apps/shell && rimraf node_modules\" \"cd apps/user-report && rimraf node_modules\" \"cd apps/user-transaction && rimraf node_modules\" \"cd apps/user-profile && rimraf node_modules\" \"cd apps/app-user && rimraf node_modules\"",
    "lint": "eslint apps/**/*.{ts,tsx} libs/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@tanstack/react-query": "^5.85.5",
    "antd": "^5.4.0",
    "axios": "^1.3.5",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@module-federation/enhanced": "^0.2.5",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "rimraf": "^4.4.1",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.2",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
